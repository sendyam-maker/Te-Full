        End If
    'Modify By Sindy 2022/12/5
    '證書形式
    Case 145
         If (KeyAscii < 49 Or KeyAscii > 50) And KeyAscii <> 8 Then
            KeyAscii = 0
            Beep
         End If
         '2022/12/5 END
    Case Else
    End Select
    
