        'Modified by Lydia 2014/12/30 代入費用一併檢查是否可收文
        Text1_Validate t1 + 1, False
        If Left(cmdOK(0).Caption, 2) = "收文" Then
           mPYchk = True
        Else
           mPYchk = False
        End If
   End If
Else
   mPYFee = False
End If
'end Add by Lydia 2014/12/15
