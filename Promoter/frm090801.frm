      adoquery.Open "select np01 from nextprogress where np02 = '" & cp01 & "' and np03 = '" & cp02 & "' and np04 = '" & cp03 & "' and np05 = '" & cp04 & "' and np06 is null and np07 = '" & CP10 & "'", cnnConnection, adOpenStatic, adLockReadOnly
