      PicRs.Open "select * from ImgByteFile where ibf05='1' and ibf01='" & strIBF01 & "' and ibf02='" & strIBF02 & "' and ibf03='" & strIBF03 & "' and ibf04='" & strIBF04 & "' ", cnnConnection, adOpenStatic, adLockOptimistic
   End If
   If PicRs.RecordCount <> 0 Then
