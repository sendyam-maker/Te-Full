                '都抓不到ZipCode
                If strZipCode = MsgText(601) Then
                    If CheckTaiwanAddr_Tai(Me.Text1(Index), Text1(119 + (Index - 11) / 16), "000", "", strZipCode, , False, Me.Name, , intTW) = False Then
                        If intTWAddr(((Index - 11) / 16) * 2) = 0 Or (intTWAddr(((Index - 11) / 16) * 2) = 10 And Left(strOldAddr(((Index - 11) / 16) * 2), 5) <> Left(Me.Text1(Index), 5)) Then
                            If MsgBox("無法依地址帶出申請人" & (Index - 11) / 16 & "申請地址" & "的郵遞區號，請問是臺灣地址嗎？", vbYesNo + vbCritical) = vbYes Then
