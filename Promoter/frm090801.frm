         Set oText1 = Text1(44)
         Set oText2 = Text1(57)
         Set oText3 = Text1(58)
      Case 4
         Set oText1 = Text1(60)
         Set oText2 = Text1(73)
         Set oText3 = Text1(74)
      Case 5
         Set oText1 = Text1(76)
         Set oText2 = Text1(89)
         Set oText3 = Text1(90)
   End Select
   If oText1.Text <> "" Then
      stCU01 = Left(oText1.Text, 8)
      stCU02 = Mid(oText1.Text, 9, 1)
