         cnnConnection.BeginTrans: bolConn = True
         
         strSql = "update FLOW002 set " & _
                  "F0205='" & strUpdDate & "'" & _
                  ",F0206='" & strUpdTime & "'" & _
                  ",F0207='1',F0204='" & strUserNum & "'" & _
                  " where F0201='" & Text5 & "' and F0202='" & strF0308 & "' and F0207 is null "
         cnnConnection.Execute strSql, intI
         
         '讀取下一處理人員
         'Modify By Sindy 2023/1/9 不可以呼叫此函數,會直接收文
         'If GetNextProPerson_Flow(Trim(Text5), Trim(strF0316), strF0308, strF0309) = False Then GoTo ErrHand
         strSql = "update FLOW003 set " & _
                  "F0307='A4'" & _
                  ",F0308='" & strUserNum & "'" & _
                  ",F0309='" & Flow_已完成 & "'" & _
                  " where F0301='" & Text5 & "' "
         cnnConnection.Execute strSql, intI
         
         cnnConnection.CommitTrans: bolConn = False
         
         Screen.MousePointer = vbDefault
      End If
   End If
   rsTmp.Close
   
   Set rsTmp = Nothing
   Exit Sub
   
ErrHand:
   Set rsTmp = Nothing
   Screen.MousePointer = vbDefault
   If bolConn = True Then
      cnnConnection.RollbackTrans
      MsgBox "簽核失敗！" & vbCrLf & Err.Description
   End If
