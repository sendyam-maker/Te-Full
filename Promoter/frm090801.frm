            intTWAddr(idx * 2 - 1) = 0: strOldAddr(idx * 2 - 1) = ""
            intTWAddr(idx * 2) = 0: strOldAddr(idx * 2) = ""
            Text1(26 + (idx - 1) * 16).Tag = MsgText(601)
