               .MoveNext
            Loop
         End If
         End With
         intQ = MsgBox(strMsg, vbQuestion + vbYesNo + vbDefaultButton1)
      End If
      
      If intQ = vbYes Then
                  
         SetPTByLC47 "" & RsQ.Fields("CRL84"), strSys, strCP10
