            Me.Combo3(Index).Tag = rsA.Fields(0).Value & " " & rsA.Fields(1).Value
         End If
      Else
          MsgBox "發明人國籍輸入錯誤!!!", vbExclamation + vbOKOnly
          Cancel = True
          Exit Sub
      End If
         
   Else
      If rsA.State <> adStateClosed Then rsA.Close
      Set rsA = Nothing
   End If
End Sub

