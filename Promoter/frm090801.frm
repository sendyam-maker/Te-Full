      Set RsTemp = ClsLawReadRstMsg(intI, strExc(0))
      If intI = 1 Then
         oText2 = "" & RsTemp.Fields(0)
         oText3 = "" & RsTemp.Fields(1)
         intTWAddr(p_index * 2 - 1) = 13 'Add by Amy 2016/01/07 抓取接洽人檔不檢查臺灣地址
      End If
   End If
End Sub

