Option Explicit
Public adoadodc1 As New ADODB.Recordset

Private Sub Combo2_KeyPress(KeyAscii As Integer)
   KeyAscii = UpperCase(KeyAscii)
End Sub

Private Sub Combo2_Validate(Cancel As Boolean)
   Combo3 = Combo2
End Sub

Private Sub Combo3_KeyPress(KeyAscii As Integer)
   KeyAscii = UpperCase(KeyAscii)
End Sub

Private Sub Form_KeyUp(KeyCode As Integer, Shift As Integer)
   KeyDefine KeyCode
End Sub

Private Sub Form_Load()
   '表單初始化
   PUB_InitForm Me, 8850, 5500
   
   strCon1 = "員工代號"
   strCon2 = "英文翻譯費率"
   strCon3 = "日文翻譯費率"
   strCon4 = "中文打字費率"
   Combo1.AddItem MsgText(31)
   Combo1.AddItem strCon1
   Combo1.AddItem strCon2
   Combo1.AddItem strCon3
   Combo1.AddItem strCon4
   Combo1 = MsgText(31)
   OpenTable
   Frmacc0000.StatusBar1.Panels(1).Text = MsgText(98)
   
End Sub

Private Sub Form_Unload(Cancel As Integer)
   If Adodc1.Recordset.RecordCount <> 0 Then
      strItemNo = Adodc1.Recordset.Fields("SPR01").Value
   Else
      strItemNo = MsgText(601)
   End If
   StatusClear
   tool1_enabled
   Frmacc11j0.Enabled = True
   Frmacc11j0.Show
   Set Frmacc11j1 = Nothing
End Sub

'*************************************************
'  功能鍵定義
'
'*************************************************
Private Sub KeyDefine(KeyCode As Integer)
   Select Case KeyCode
      Case vbKeyF12
         Screen.MousePointer = vbHourglass
         QueryData
         Screen.MousePointer = vbDefault
   End Select
   KeyEnter KeyCode
   Frmacc0000.StatusBar1.Panels(1).Text = MsgText(98)
End Sub

'*************************************************
'  票據資料查詢
'
'*************************************************
Private Sub QueryData()
On Error GoTo Checking
   adoadodc1.Close
   adoadodc1.CursorLocation = adUseClient
   Select Case Combo1
      Case strCon1
         strCondition = "SPR01"
      Case strCon2
         strCondition = "SPR02"
      Case strCon3
         strCondition = "SPR03"
      Case strCon4
         strCondition = "SPR04"
      Case MsgText(31)
         adoadodc1.Open "select SPR01,ST02,SPR02,SPR03,SPR04 from STAFF_PAYRATE,STAFF WHERE ST01=SPR01 order by SPR01 asc", adoTaie, adOpenStatic, adLockReadOnly
         Adodc1.Recordset.Requery
         Exit Sub
      Case Else
         Exit Sub
   End Select
   If Combo3 = MsgText(601) Then
      adoadodc1.Open "select SPR01,ST02,SPR02,SPR03,SPR04 from STAFF_PAYRATE,STAFF where ST01=SPR01 AND " & strCondition & " like '" & "%" & Combo2 & "%" & "' order by " & strCondition & " asc", adoTaie, adOpenStatic, adLockReadOnly
   Else
      If Combo2 = MsgText(601) Then
         adoadodc1.Open "select SPR01,ST02,SPR02,SPR03,SPR04 from STAFF_PAYRATE,STAFF where ST01=SPR01 AND " & strCondition & " <= '" & Combo3 & "' order by " & strCondition & " asc", adoTaie, adOpenStatic, adLockReadOnly
      Else
         adoadodc1.Open "select SPR01,ST02,SPR02,SPR03,SPR04 from STAFF_PAYRATE,STAFF where ST01=SPR01 AND " & strCondition & " >= '" & Combo2 & "' and " & strCondition & " <= '" & Combo3 & "' order by " & strCondition & " asc", adoTaie, adOpenStatic, adLockReadOnly
      End If
   End If
   Adodc1.Recordset.Requery
   If Adodc1.Recordset.RecordCount = 0 Then
      MsgBox MsgText(33), , MsgText(5)
   End If
   Exit Sub
Checking:
   If Err.Number = 0 Then
      Exit Sub
   End If
   MsgBox Err.Description, , MsgText(5)
End Sub

'*************************************************
'  開啟資料表
'
'*************************************************
Private Sub OpenTable()
On Error GoTo Checking
   adoadodc1.CursorLocation = adUseClient
   adoadodc1.Open "select SPR01,ST02,SPR02,SPR03,SPR04 from STAFF_PAYRATE,STAFF where ST01=SPR01 AND ROWNUM<1", adoTaie, adOpenStatic, adLockReadOnly
   Set Adodc1.Recordset = adoadodc1
Checking:
   If Err.Number = 0 Then
      Exit Sub
   End If
   MsgBox Err.Description, , MsgText(5)
End Sub




