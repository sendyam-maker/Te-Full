'Added by Morgan 2021/6/25 刪除使用者印表機註冊機碼
Public Function PUB_DelPrinter(ByVal strName As String) As Boolean
Dim keyhand As Long, r As Long
   r = RegOpenKey(HKEY_CURRENT_USER, "Software\Microsoft\Windows NT\CurrentVersion\Devices", keyhand)
   If r = ERROR_SUCCESS And keyhand <> 0 Then
      r = regDeleteValue(keyhand, strName)
      If r = ERROR_SUCCESS Then
         PUB_DelPrinter = True
      End If
   End If
   If keyhand <> 0 Then r = RegCloseKey(keyhand)
End Function
